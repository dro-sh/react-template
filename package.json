{
  "name": "home-react",
  "version": "0.0.1",
  "description": "Booker Home-React application",
  "scripts": {
    "dev": "cross-env NODE_ENV=development PORT=8001 concurrently --names tsc,esb -c green.bold,blue.bold --kill-others \"npm run bundle -- --dev\" \"tsc --watch\" ",
    "build": "cross-env NODE_ENV=production PORT=8001 concurrently --success --names tsc,esb -c green.bold,blue.bold tsc \"npm run bundle\" ",
    "bundle": "node ./esbuild.config.mjs",
    "clean": "rimraf public/dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "concurrently": "^7.2.2",
    "esbuild": "^0.14.48",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fontsource/roboto": "^4.5.7",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "5.8.4",
    "create-serve": "^1.0.1",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0"
  }
}
